using System;
using Models.Core;
using Models.PMF;
using APSIM.Shared.Utilities;

namespace Models
{
    [Serializable]
    public class Script : Model
    {
        [Link] Clock Clock;
        [Link] Fertiliser Fertiliser;
        [Link] Summary Summary;
        
        [Description("Day of year fertiliser is to be applied")]
        public int FertDayOfYear { get; set;}

        [Description("Amount of fertiliser to be applied")]
        public double Amount { get; set;}

        [EventSubscribe("DoManagement")]
        private void OnDoManagement(object sender, EventArgs e)
        {
            if (Clock.Today.DayOfYear == FertDayOfYear)
            {
                Fertiliser.Apply(Amount: Amount, Type: Fertiliser.Types.NO3N);
            }
        
        }
        
    }
}

