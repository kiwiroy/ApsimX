[EventSubscribe("Initialised")]
private void OnInitialised(object sender, EventArgs e)
{
InitialDay = true;
AccumPlantN = 0;
AccumMineralisation = 0;
AccumDenit = 0;
AccumFert = 0;
AccumLeach = 0;
AccumDetach = 0;
DeltaSoilOMN = 0;
DeltaSurfaceOMN = 0;
DeltaSoilMineralN = 0;
