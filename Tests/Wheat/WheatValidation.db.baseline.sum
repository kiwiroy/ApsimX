AccumLeach = 0;
AccumDetach = 0;
DeltaSoilOMN = 0;
DeltaSurfaceOMN = 0;
DeltaSoilMineralN = 0;
SoilNitrogenContent = 0;

AccumEO = 0;
AccumEP = 0;
