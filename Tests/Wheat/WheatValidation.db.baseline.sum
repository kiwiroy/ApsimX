[EventSubscribe("DoManagementCalculations")]
private void OnDoManagementCalculations(object sender, EventArgs e)
{
    int layers = Soil.Thickness.Length;
    if (InitialDay)
    {
      InitialSurfaceOMN = SOM.N;
      for (int layer = 0; layer < layers; layer++)
      { 
      InitialSoilOMN += (Soil.SoilNitrogen.MicrobialN[layer] + Soil.SoilNitrogen.FOMN[layer] + Soil.SoilNitrogen.HumicN[layer]);
      InitialMineralN += (Soil.NO3N[layer] + Soil.NH4N[layer] + Soil.SoilNitrogen.urea[layer]);
      }
      InitialDay = false;
    }

//!!!!!! Nitrogen balance components !!!!!!
//!!!!!!^^^^^^^^^^^^^^^^^^^^^^^^^^^^^!!!!!!
//AccumPlantN -= (double)this.Get("[Plant].Root.NUptake");
