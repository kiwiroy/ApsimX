AccumLeach = 0;
AccumDetach = 0;
DeltaSoilOMN = 0;
DeltaSurfaceOMN = 0;
DeltaSoilMineralN = 0;
SoilNitrogenContent = 0;

AccumEO = 0;
AccumEP = 0;
AccumES = 0;
AccumDrainage = 0;
AccumRunoff = 0;
AccumOutflowLat = 0;
AccumIrrigation = 0;
AccumRainfall = 0;
SoilWaterDeficit = 0;

AccumTtemerg = 0;

SWC = 0;
DUL = 0;
}       

[EventSubscribe("DoManagementCalculations")]
private void OnDoManagementCalculations(object sender, EventArgs e)
{
    int layers = Soil.Thickness.Length;
    if (InitialDay)
    {
      InitialSurfaceOMN = SOM.N;
      for (int layer = 0; layer < layers; layer++)
      { 
      InitialSoilOMN += (Soil.SoilNitrogen.MicrobialN[layer] + Soil.SoilNitrogen.FOMN[layer] + Soil.SoilNitrogen.HumicN[layer]);
      InitialMineralN += (Soil.NO3N[layer] + Soil.NH4N[layer] + Soil.SoilNitrogen.urea[layer]);
      }
      InitialDay = false;
    }

//!!!!!! Nitrogen balance components !!!!!!
//!!!!!!^^^^^^^^^^^^^^^^^^^^^^^^^^^^^!!!!!!
//AccumPlantN -= (double)this.Get("[Plant].Root.NUptake");
AccumFert += Fert.NitrogenApplied;
DeltaSurfaceOMN = SOM.N - InitialSurfaceOMN;
AccumDetach = 0; //plant2 doesn't detach yet
MineralN = 0;
TotalNO3 = 0;
TotalNH4 = 0;
double OrganicN = 0;
//AccumLeach = AccumLeach - Soil.SoilNitrogen.leach_urea - Soil.SoilNitrogen.leach_no3 - Soil.SoilNitrogen.leach_nh4; //Fails
   for (int layer = 0; layer < layers; layer++)
    { 
    AccumMineralisation += (Soil.SoilNitrogen.MineralisedN[layer] + Soil.SoilNitrogen.dlt_n_min_res[layer]);
    AccumDenit -= MathUtilities.Sum(Soil.SoilNitrogen.Denitrification);
    OrganicN += (Soil.SoilNitrogen.MicrobialN[layer] + Soil.SoilNitrogen.FOMN[layer] + Soil.SoilNitrogen.HumicN[layer]);
    MineralN += (Soil.NO3N[layer] + Soil.NH4N[layer] + Soil.SoilNitrogen.urea[layer]);
    TotalNO3 += Soil.NO3N[layer];
    TotalNH4 += Soil.NH4N[layer];
    }
DeltaSoilOMN = OrganicN - InitialSoilOMN;
DeltaSoilMineralN = MineralN - InitialMineralN;
SoilNitrogenContent = MineralN;
//!!!!!! Water balance components !!!!!!
//!!!!!!^^^^^^^^^^^^^^^^^^^^^^^^^^!!!!!!
AccumEO -= Soil.SoilWater.Eo;
//AccumEP -= (double)this.Get("[Plant].Leaf.Transpiration");
AccumES -= Soil.SoilWater.Es; 
AccumDrainage -= Soil.SoilWater.Drainage; 
AccumRunoff -= Soil.SoilWater.Runoff; 
AccumIrrigation += Irrig.IrrigationApplied; 
AccumRainfall += Met.Rain;
SWC = 0;
DUL = 0;
double OutFlowLat = 0;
   for (int layer = 0; layer < layers; layer++)
   {
     SWC += Soil.SoilWater.SWmm[layer];
     DUL += Soil.SoilWater.DULmm[layer];
     OutFlowLat = Soil.SoilWater.outflow_lat[layer];
   }
AccumOutflowLat -= OutFlowLat;
SoilWaterDeficit =  (SWC - DUL);
}

